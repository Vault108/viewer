<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel
 border="true"
 follows="top|left"
 height="500"
 help_topic=""
 label="CEF Plugin Tab"
 layout="topleft"
 left="0"
 name="CEF Plugin Tab"
 width="820">
	<panel.string
     name="home_page_url">
		https://sl-viewer-media-system.s3.amazonaws.com/bookmarks.html
	</panel.string>
	<layout_stack	 
     bottom="496"
     follows="left|right|top|bottom"
     layout="topleft"
     left="4"
     name="stack1"
     orientation="vertical"
     top="4"
     width="816">
		<layout_panel
         auto_resize="false"
         default_tab_group="1"
         height="20"
         layout="topleft"
         left="0"
         min_height="20"
         name="nav_controls"
         top="400"
         width="800">
			<button
             follows="left|top"
             height="20"
             label="Back"
             layout="topleft"
             left="0"
             name="back_btn"
             top="0"
             width="55">
			</button>
			<button
             follows="left|top"
             height="20"
             label="Forward"
             layout="topleft"
             left_pad="3"
             name="forward_btn"
             top_delta="0"
             width="68">
			</button>
			<button
             enabled="true"
             follows="left|top"
             height="20"
             label="Reload"
             layout="topleft"
             left_pad="2"
             name="reload_btn"
             top_delta="0"
             width="70">
			</button>
			<combo_box
             allow_text_entry="true"
             follows="left|top|right"
             tab_group="1"
             height="20"
             layout="topleft"
             left_pad="5"
             max_chars="20000"
             name="address"
             combo_editor.select_on_focus="true"
             top_delta="0"
             width="540">
			</combo_box>
			<button
             enabled="true"
             follows="right|top"
             height="20"
             label="Go"
             layout="topleft"
             left_pad="5"
             name="go_btn"
             top_delta="0"
             width="50">
			</button>
		</layout_panel>
		<layout_panel
         height="800"
         layout="topleft"
         left="0"
		 right="0"
         name="browser_panel"
         top="0">
			<web_browser
			   bg_opaque_color="0.65 0.35 0.35 1"
               background_opaque="true"
			   height="800"
			   follows="all"
			   layout="topleft"
			   name="browser"
			   top="0" />
		</layout_panel>
	</layout_stack>
</panel>
